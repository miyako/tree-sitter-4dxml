{
  "name": "tree-sitter-4dxml",
  "version": "0.5.0",
  "description": "Atom language support for 4D",
  "main": "index.js",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miyako/tree-sitter-4dxml.git"
  },
  "tree-sitter": [
    {
      "scope": "4dxml",
      "file-types": [
        "4dcatalog",
        "4dlink",
        "4dpreferences",
        "4dsettings"
      ]
    }
  ],
  "keywords": [
    "4D",
    "ORDA",
    "tree-sitter"
  ],
  "authors": [
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>",
    "Keisuke Miyako <keisuke.miyako@4d.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miyako/tree-sitter-4dxml/issues"
  },
  "homepage": "https://github.com/miyako/tree-sitter-4dxml#readme",
  "dependencies": {
    "nan": "^2.17.0",
    "node-gyp": "^9.0.0",
    "npm-check-updates": "^13.1.5"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.9"
  }
}
